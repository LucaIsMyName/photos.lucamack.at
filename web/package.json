{
  "name": "web",
  "private": true,
  "version": "1.4.31",
  "type": "module",
  "description": "Photo Gallery & Notebook with Node, Vite, React & Tailwind.",
  "author": "Luca Mack",
  "license": "MIT",
  "homepage": "https://photos.lucamack.at",
  "repository": {
    "type": "git",
    "url": "https://github.com/LucaIsMyName/photos.lucamack.at.git"
  },
  "bugs": {
    "url": "https://github.com/LucaIsMyName/photos.lucamack.at/issues"
  },
  "scripts": {
    "dev": "vite",
    "clean": "rimraf dist",
    "build": "npm run clean && npm run generate && tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "predev": "npm run generate",
    "generate": "npm run generate:galleries && npm run generate:pages && npm run generate:sitemap",
    "generate:galleries": "node generate-galleries.mjs",
    "generate:pages": "node generate-pages.mjs",
    "generate:sitemap": "node generate-sitemap.mjs",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "scan": "react-scan",
    "scan:components": "react-scan --analysis components",
    "scan:hooks": "react-scan --analysis hooks",
    "scan:performance": "react-scan --analysis performance"
  },
  "dependencies": {
    "@mapbox/mapbox-sdk": "^0.16.1",
    "@math.gl/web-mercator": "^4.1.0",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slider": "^1.3.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@types/react-map-gl": "^6.1.7",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "framer-motion": "^12.23.0",
    "html-to-image": "^1.11.13",
    "lucide-react": "^0.525.0",
    "mapbox-gl": "^3.13.0",
    "marked": "^16.0.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-intersection-observer": "^9.16.0",
    "react-map-gl": "^8.0.4",
    "react-router-dom": "^7.6.3",
    "recharts": "^3.1.0",
    "supercluster": "^8.0.1",
    "tailwind-merge": "^3.3.1",
    "use-supercluster": "^1.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@eslint/js": "^9.29.0",
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.1.11",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/mapbox__mapbox-sdk": "^0.16.3",
    "@types/mapbox-gl": "^3.4.1",
    "@types/marked": "^5.0.2",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.5.2",
    "autoprefixer": "^10.4.21",
    "babel-plugin-react-compiler": "^19.1.0-rc.2",
    "eslint": "^9.29.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "exiftool-vendored": "^30.3.0",
    "globals": "^16.2.0",
    "happy-dom": "^18.0.1",
    "heic-convert": "^2.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jsdom": "^26.1.0",
    "postcss": "^8.5.6",
    "react-scan": "^0.4.3",
    "rimraf": "^6.0.1",
    "rollup-plugin-visualizer": "^6.0.3",
    "sharp": "^0.33.5",
    "tailwindcss": "^4.1.11",
